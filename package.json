{
  "name": "react-webpack-babel",
  "version": "1.0.0",
  "description": "Simple boilerplate for React, Webpack, Babel, Enzyme",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --watch",
    "test": "jest",
    "build": "webpack-dev-server -d --watch",
    "build:prod": "webpack -p"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "author": "Lukasz Andruchow",
  "license": "ISC",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^2.9.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.0.2",
    "path": "^0.12.7",
    "react-scripts": "^1.0.13",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
